[snapshot]
snapshot_dir = /var/lib/teefaa/snapshots
exclude_dirs = local/*

[repository]
key_name = yourkey
key_path = /home/youraccount/.ssh/id_dsa
repo_image = ami-000000d6

[partitioning]
disk = sda
sizes = | 2    | 50   | -1
types = | swap | ext4 | xfs
mount = | none | /    | /data

[image]
image_repo = 192.168.16.1:/, 192.168.16.2:/mnt/snapshot
diffs = /var/lib/teefaa/diffs
